{
  "name": "instagram-inbox-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node --inspect src -r dotenv/config \" --ignore src/cookies --ignore test/",
    "build": "babel --require dotenv/config src -s -D -d dist",
    "prestart": "npm run -s build",
    "test": "eslint src && ava -v test/**/*__test.js",
    "watch": "ava -v --watch test/**/*__test.js"
  },
  "keywords": [
    "instagram",
    "inbox",
    "micro",
    "private",
    "mailbox"
  ],
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "instagram-private-api": "^0.5.16",
    "lodash": "^4.17.4",
    "micro": "^7.3.3",
    "micro-cors": "^0.0.4",
    "microrouter": "^2.1.1",
    "ava": "^0.19.1",
    "request-promise": "^4.2.1",
    "rewire": "^2.5.2",
    "test-listen": "^1.0.2"
  },
  "ava": {
    "require": [
      "babel-register",
      "dotenv/config"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0"
  }
}
